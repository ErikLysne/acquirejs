    "@types/jest": "^29.5.0",
    "@types/mersenne-twister": "^1.1.2",
    "axios-mock-adapter": "^1.21.4",
    "chance": "^1.1.11",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
